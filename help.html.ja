	}
}
function set_strand(){
	var strand ;
	if (location.pathname.match(/\/(\+|\-)\/help.html$/)){
		strand = location.pathname.match(/\/(\+|\-)\/help.html$/)[1] ;
		if (strand == '+'){
			document.gggenome.strand[1].checked = true ;
