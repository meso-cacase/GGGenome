		document.gggenome.db.value = db ;
		if (document.gggenome.db.selectedIndex < 0){
			document.gggenome.db.value = 'hg19' ;
		}
	}
}
function set_k(){
	var k ;
	if (location.pathname.match(/\/(\d+)\/((\+|\-)\/)?help.html$/)){
		k = location.pathname.match(/\/(\d+)\/((\+|\-)\/)?help.html$/)[1] ;
		document.gggenome.k.value = k ;
	}
}
function set_strand(){
	var strand ;
	if (location.pathname.match(/\/(\+|\-)\/help.html$/)){
		strand = location.pathname.match(/\/(\+|\-)\/help.html$/)[1] ;
		if (strand == '+'){
			document.gggenome.strand[1].checked = true ;
		} else if (strand == '-'){
			document.gggenome.strand[2].checked = true ;
		}
	}
}
//-->
